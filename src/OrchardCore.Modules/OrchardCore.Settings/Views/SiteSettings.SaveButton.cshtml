<button role="button save" class="primaryAction btn btn-primary" type="submit">@T["Save"]</button>

@{
    var returnUrl = Context.Request.Query["returnUrl"];
}

@if (!String.IsNullOrWhiteSpace(returnUrl) && Url.IsLocalUrl(returnUrl))
{
    <a role="button cancel" href="@returnUrl" class="btn btn-secondary">@T["Cancel"]</a>
}
